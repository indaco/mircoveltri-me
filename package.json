{
	"name": "sveltin",
	"version": "0.0.1",
	"packageManager": "pnpm@8.7.1",
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"clean": "rimraf .svelte-kit build",
		"clean:all": "rimraf .svelte-kit node_modules build",
		"build": "npm run clean && svelte-kit sync && vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@indaco/svelte-iconoir": "^4.6.0",
		"@sveltejs/adapter-static": "2.0.3",
		"@sveltejs/kit": "^1.24.1",
		"@sveltinio/remark-headings": "^1.0.3",
		"@sveltinio/seo": "^0.4.2",
		"@sveltinio/services": "^0.4.2",
		"@sveltinio/ts-utils": "^0.8.0",
		"@tailwindcss/typography": "^0.5.9",
		"@types/lodash": "^4.14.197",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"autoprefixer": "^10.4.15",
		"cssnano": "^5.1.15",
		"dayjs": "^1.11.9",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.33.0",
		"lodash": "^4.17.21",
		"mdast-util-to-string": "^3.2.0",
		"mdsvex": "^0.11.0",
		"mdsvex-relative-images": "^1.0.3",
		"postcss": "^8.4.29",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-external-links": "^2.1.0",
		"rehype-slug": "^5.1.0",
		"remark-emoji": "^3.1.2",
		"rimraf": "^5.0.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"unist-util-visit": "^4.1.2",
		"vite": "^4.4.9"
	},
	"type": "module"
}
